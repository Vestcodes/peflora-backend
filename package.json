{
  "name": "backend",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "setup-admin": "node setup-admin.js",
    "setup-products": "node setup-products-standalone.js",
    "setup-products-cli": "node setup-products-cli.js",
    "setup-products-framework": "medusa exec ./src/scripts/setup-products-framework.ts",
    "start": "NODE_ENV=production medusa start --port ${PORT}",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.2",
    "@medusajs/cli": "2.10.2",
    "@medusajs/event-bus-redis": "^2.10.2",
    "@medusajs/framework": "2.10.2",
    "@medusajs/medusa": "2.10.2",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@perseidesjs/notification-nodemailer": "^1.1.0",
    "@prisma/client": "6.17.1",
    "@rsc-labs/medusa-documents-v2": "^0.2.11",
    "@rsc-labs/medusa-products-bought-together-v2": "^0.0.3",
    "@rsc-labs/medusa-wishlist": "^0.0.3",
    "awilix": "^8.0.1",
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "isomorphic-fetch": "^3.0.0",
    "medusa-plugin-razorpay-v2": "^0.1.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "pg": "^8.13.0",
    "prisma": "^6.17.1",
    "razorpay": "^2.9.6",
    "tsx": "^4.20.6",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.2",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
